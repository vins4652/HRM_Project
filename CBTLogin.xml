<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Loginpage_CBT" parallel="tests" thread-count="2">
	<test name="ChromeBrowserTesting">
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="login_Module.Login_as_Assistant" />
			<class name="login_Module.Login_as_Head" />
			<class name="login_Module.Login_as_Officer" />
		</classes>
	</test>
	<test name="FirefoxBrowserTesting">
		<parameter name="browserName" value="firefox"></parameter>
		<classes>
			<class name="login_Module.Login_as_Assistant" />
			<class name="login_Module.Login_as_Head" />
			<class name="login_Module.Login_as_Officer" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Loginpage_CBT -->
